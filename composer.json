{
    "name": "siganushka/order-bundle",
    "description": "Order Bundle.",
    "keywords": ["order-bundle"],
    "homepage": "https://github.com/siganushka/order-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "siganushka",
            "email": "siganushka@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "knplabs/knp-components": "^5.0",
        "siganushka/doctrine-contracts": "dev-main",
        "siganushka/generic-bundle": "dev-main",
        "symfony/doctrine-bridge": "^7.4|^8.0",
        "symfony/form": "^7.4|^8.0",
        "symfony/framework-bundle": "^7.4|^8.0",
        "symfony/validator": "^7.4|^8.0",
        "symfony/workflow": "^7.4|^8.0"
    },
    "require-dev": {
        "godruoyi/php-snowflake": "^3.0",
        "symfony/messenger": "^7.4|^8.0",
        "symfony/translation": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Siganushka\\OrderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siganushka\\OrderBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tools:run": [
            "tools/vendor/bin/php-cs-fixer fix",
            "tools/vendor/bin/simple-phpunit",
            "tools/vendor/bin/phpstan"
        ]
    }
}
